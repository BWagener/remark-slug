{
  "name": "remark-slug",
  "version": "3.0.0",
  "description": "Add anchors to remark heading nodes",
  "license": "MIT",
  "keywords": [
    "text",
    "markdown",
    "slug",
    "id",
    "github",
    "npm",
    "remark"
  ],
  "dependencies": {
    "mdast-util-to-string": "^1.0.0",
    "unist-util-visit": "^1.0.0",
    "repeat-string": "^1.5.0",
    "slugg": "^0.1.0"
  },
  "repository": "wooorm/remark-slug",
  "author": "Titus Wormer <tituswormer@gmail.com>",
  "files": [
    "index.js"
  ],
  "devDependencies": {
    "browserify": "^11.0.0",
    "eslint": "^1.0.0",
    "esmangle": "^1.0.0",
    "istanbul": "^0.3.0",
    "jscs": "^2.0.0",
    "jscs-jsdoc": "^1.0.0",
    "remark": "^3.0.0",
    "remark-comment-config": "^2.0.0",
    "remark-github": "^2.0.0",
    "remark-html": "^2.0.0",
    "remark-lint": "^2.0.0",
    "remark-usage": "^2.0.0",
    "remark-validate-links": "^2.0.0",
    "mocha": "^2.0.0",
    "slug": "^0.9.0",
    "to-slug-case": "^0.1.0"
  },
  "scripts": {
    "test-api": "mocha --check-leaks test.js",
    "test-coverage": "istanbul cover _mocha -- test.js",
    "test-travis": "npm run test-coverage",
    "test": "npm run test-api",
    "lint-api": "eslint .",
    "lint-style": "jscs --reporter inline .",
    "lint": "npm run lint-api && npm run lint-style",
    "make": "npm run lint && npm run test-coverage",
    "bundle": "browserify index.js --no-builtins -s remarkSlug > remark-slug.js",
    "postbundle": "esmangle remark-slug.js > remark-slug.min.js",
    "build-md": "remark . --quiet --frail",
    "build": "npm run bundle && npm run build-md"
  }
}
